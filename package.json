  "repository": {
    "type": "git",
    "url": "git+https://github.com/thenolle/thenolle.com.git"
  },
  "main": "dist/index.js",
  "scripts": {
    "build": "rm -rf dist && webpack --mode production && echo 'RewriteEngine On\n\n# If an existing asset or directory is requested go to it as it is\nRewriteCond %{DOCUMENT_ROOT}%{REQUEST_URI} -f [OR]\nRewriteCond %{DOCUMENT_ROOT}%{REQUEST_URI} -d\nRewriteRule ^ - [L]\n\n# If the requested resource does not exist, use index.html\nRewriteRule ^ /index.html\n\n\n# Â© 2023 Nolly | Website: thenolle.com | Github: @TheNolle | Patreon: @_nolly' > dist/.htaccess",
    "start": "webpack serve --mode production --config webpack.config.mjs --no-cache",
    "start:dev": "webpack serve --mode development --config webpack.config.mjs --no-cache"
  },
  "dependencies": {
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "react-router-dom": "^6.23.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.4",
    "@babel/preset-env": "^7.24.4",
    "@babel/preset-react": "^7.24.1",
    "@babel/preset-typescript": "^7.24.1",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "babel-loader": "^9.1.3",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^7.1.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.6.0",
    "sass": "^1.75.0",
    "sass-loader": "^14.2.1",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "url-loader": "^4.1.1",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4"
  }
}